
<% if(errors.length > 0) { %>
<div class="row" style="padding: 0px 0px 0px 18px;">
    <div class="col">
        <br>
      <div class="alert alert-danger">
        <% errors.forEach(erro => { %>
        <%= erro %><br>
        <% } ); %>
      </div>
    </div>
</div>
<% }; %>

<% if(success.length > 0) { %>
    <div class="row" style="padding: 0px 0px 0px 18px;">
        <div class="col">
            <br>
          <div class="alert alert-success">
            <% success.forEach(success => { %>
            <%= success %><br>
            <% } ); %>
          </div>
        </div>
    </div>
    <% }; %>